#include <iostream>

int main() {
    int count = 0;

    for (int num = 100; num <= 999; ++num) {
        int hundreds = num / 100;
        int tens = (num / 10) % 10;
        int units = num % 10;

        if (hundreds == tens || tens == units || hundreds == units) {
            count++;
        }
    }

    std::cout << "Number of three-digit integers with two identical digits: " << count << std::endl;

    return 0;
}
